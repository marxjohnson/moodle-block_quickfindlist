{{!
    @template block_quickfindlist/results

    Context variables required for this template:
    * roleid - User role for these results
    * users - Array of users, {url, displayname}

    Example context (json):
    {
        "roleid": "-1",
        "users": [
            {
                "url": "https://example.com/user.php?id=1",
                "displayname": "Aaron Aaronson"
            },
            {
                "url": "https://example.com/user.php?id=2",
                "displayname": "Bob Bobson"
            }
        ]
    }
}}

<ul id="quickfindlist{{roleid}}" class="quickfindlistresults">
    {{#users}}
        <li>
            <a href="{{url}}">{{displayname}}</a>
        </li>
    {{/users}}
</ul>